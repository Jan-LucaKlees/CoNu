{
  "name": "CoNu",
  "version": "2.1.1",
  "description": "A game about connecting numbers.",
  "main": "index.html",
  "scripts": {
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "dev": "npm run dev:official-website & npm run dev:github-pages",
    "_dev-script": "webpack-dev-server --config ./webpack.config.js --mode development --devtool source-map",
    "dev:official-website": "npm run _dev-script --",
    "dev:github-pages": "npm run _dev-script -- --env.BUILD_FOR_GITHUB_PAGES=true --port 8081",
    "preview": "npm run preview:official-website",
    "preview:official-website": "npm run build:official-website -- --env.PREVIEW=true && firebase serve --only hosting",
    "build": "npm run build:official-website && npm run build:github-pages",
    "_build-script": "webpack --config ./webpack.config.js --mode production",
    "build:official-website": "rm -rf public && npm run _build-script --",
    "build:github-pages": "rm -rf docs   && npm run _build-script -- --env.BUILD_FOR_GITHUB_PAGES=true",
    "deploy": "npm run test && npm run build && firebase deploy"
  },
  "author": "Jan-Luca Klees",
  "license": "GNU GPLv3",
  "dependencies": {
    "classnames": "^2.2.6",
    "firebase": "^6.3.3",
    "minireset.css": "0.0.5",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-transition-group": "^4.2.1",
    "uuid": "^3.3.2",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "jest-junit": "^6.4.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "react-hot-loader": "^4.12.10",
    "sass": "^1.22.7",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  }
}
